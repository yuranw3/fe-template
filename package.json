{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "no description",
  "BU": "{{bu}}",
  "author": "Forcs Zhang",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "prebuild": "npm install",
    "build": "node build/build.js",
    "uat": "npm install&&node build/uat.js",
